{
  "name": "generator-typescript-project",
  "version": "1.0.0",
  "description": "Yeoman typescript generator, includes jest, prettier and standard-version",
  "keywords": [
    "typescript",
    "conventional-commits",
    "prettier",
    "yeoman-generator"
  ],
  "license": "MIT",
  "author": "",
  "files": [
    "generators"
  ],
  "scripts": {
    "prebuild": "rm -rf generators",
    "build": "tsc && cp -r src/app/templates generators/app/templates",
    "prepare": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "arr-union": "^3.1.0",
    "uniqs": "^2.0.0",
    "yeoman-generator": "^3.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@types/arr-union": "^3.1.0",
    "@types/uniq": "^0.0.27",
    "@types/yeoman-generator": "^3.0.0",
    "husky": "^1.1.3",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.8.0",
    "standard-version": "^4.4.0",
    "typescript": "^3.1.6"
  }
}
